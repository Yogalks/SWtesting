//program7
package Task;

public class task7 {

	public static void main(String[] args) {
		 String str = "Guvi Geek";
		 int count =0;
		
		 for(int i=0; i<str.length(); i++)
		 {
			 if(str.charAt(i)!=' ')
			 {
				 count++;
			 }
			 
				 
		 }
        
        System.out.println("The total count of the word is" + " " + count);
        
}
}
